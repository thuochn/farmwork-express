extends ../layouts/common

block content
	h1 List Users
	a(href="users/create") Create Users
	form(action="/users/search", method= "GET")
		input(type= "text", class="form-control", name= "q")
		button(type="submit", class="btn btn-primary mt-2") search
	table.table
		tr
			th Name
			th
		each user in users
			tr
				td= user.name
				td
					a(href="/users/view/" + user.id) xem chi tiet